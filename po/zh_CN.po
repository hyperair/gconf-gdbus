msgstr "将 GConf 树保存到“%s”时发生错误：%s\n"

#: backends/gconf-merge-tree.c:121
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "用法：%s <dir>\n"

#: backends/gconf-merge-tree.c:127
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"用法：%s <dir>\n"
"  合并一个标记后端文件系统层次形如：\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  成为：\n"
"    dir/%%gconf-tree.xml\n"

