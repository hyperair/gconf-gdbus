#: gconf/gconf-backend.c:167
#, c-format
msgid "No such file `%s'\n"
msgstr "Aucun fichier « %s »\n"

#: gconf/gconf-backend.c:195
#, c-format
msgid "Bad address `%s'"
msgstr "Adresse incorrecte « %s »"

