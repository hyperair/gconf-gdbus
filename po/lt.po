msgstr "Klaida saugant GConf medį į „%s“: %s\n"

#: backends/gconf-merge-tree.c:101
#, c-format
msgid "Usage: %s <dir>\n"
msgstr "Naudojimas: %s <dir>\n"

#: backends/gconf-merge-tree.c:107
#, c-format
msgid ""
"Usage: %s <dir>\n"
"  Merges a markup backend filesystem hierarchy like:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  to:\n"
"    dir/%%gconf-tree.xml\n"
msgstr ""
"Naudojimas: %s <dir>\n"
"  Sujungia pažymėto backend'o bylų sitemos hierarchiją kaip:\n"
"    dir/%%gconf.xml\n"
"        subdir1/%%gconf.xml\n"
"        subdir2/%%gconf.xml\n"
"  į:\n"
"    dir/%%gconf-tree.xml\n"

